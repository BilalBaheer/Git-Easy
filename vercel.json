{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://git-easy-git-main-bilalbaheers-projects.vercel.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "https://git-easy-git-main-bilalbaheers-projects.vercel.app"
  }
}
